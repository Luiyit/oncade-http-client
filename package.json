{
  "name": "oncade-http-client",
  "version": "0.1.6",
  "description": "A TypeScript API client for Oncade",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.esm.js"
    },
    "./jest": {
      "types": "./dist/new-file.d.ts",
      "require": "./dist/new-file.cjs",
      "import": "./dist/new-file.esm.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "jest ./src",
    "build": "yarn removeDist && rollup -c",
    "buildPublish": "yarn build && yarn publish",
    "publish": "npm publish",
    "addUser": "npm adduser",
    "removeDist": "rimraf dist",
    "cleanCache": "yarn cache clean && npm cache clean --force",
    "watch": "npm run build -- --watch src"
  },
  "keywords": [
    "oncade",
    "api",
    "client",
    "typescript",
    "axios"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Luiyit/oncade-http-client.git"
  },
  "bugs": {
    "url": "https://github.com/Luiyit/oncade-http-client/issues"
  },
  "author": {
    "name": "Luiyit Hernandez",
    "email": "luiyit.hernandez@widget-pc.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.1",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-typescript": "12.1.1",
    "@swc/core": "1.10.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.0.0",
    "jest": "29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsup": "8.3.5",
    "tsx": "4.20.5",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "axios": "1.7.0",
    "dotenv": "17.2.2"
  }
}
